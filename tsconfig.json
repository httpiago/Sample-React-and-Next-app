{
  "compilerOptions": {
    "rootDir": "src",
    "target": "es6",
    "lib": ["es6", "dom"],
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "sourceMap": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noImplicitAny": false,
  },
  "exclude": [
    "node_modules",
    "dist",
    ".cache"
  ]
}